package Study;

interface MathOperation
{
	int operation(int a,int b);
}

public class Demo  
{
	public static void main(String[] args) 
	{
		 MathOperation add=(a,b)->a+b;
		 MathOperation multiply=(a,b)->a*b;
		 System.out.println("Addition of 2 nos is "+add.operation(5,6));
		 System.out.println("Multiplication of 2 nos is "+multiply.operation(5,6));;
	}

}

		
	


